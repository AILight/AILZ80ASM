                              *** AILZ80ASM *** Z-80 Assembler, version 0.9.4.0
000000 9000                       ORG $9000
                              
                              ADDR:
                                  REGTEST HL
000000 9000 00             4 +    NOP
                                  REGTEST ADDR
000001 9001 210090        10 +    ld hl, target
                                  ABC(DEF)
000004 9004 AF             4 +    XOR A
                              
                              ; レジスター指定テスト
                              MACRO REGTEST target
                              #IF target.@Text != "HL"
                                  ld hl, target
                              #ELSE
                                  NOP
                              #ENDIF
                              END MACRO
                              
                              ; カッコつきマクロテスト
                              MACRO ABC(DEF)
                                  XOR A
                              END MACRO
[EOF:Test.Z80:UTF_8]
